<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ../partials/head%>
    <style>
      
    </style>
  </head>
  <body>
    <%- include ../partials/nav%>
    <div id="container" class="fluid">
      <main role="main">
        <%- include ../partials/jumbotron%>
        <div class="container">
            
            
          <div class="row"> 
              <% items.forEach(function(item){ %>
            <div class="col-md-4">
                    <div class="card">
                  <img src="<%= item.src %>"/>
                <div class="card-body">
                  <h5 class="card-title"><%= item.name %></h5>
                  <p class="card-text"><%= item.text %></p>
                </div>
              </div>
            </div>
            <div class="col-md-4">
              <div class="card opening-card">
                <img
                  src="<%= item.src %>"
                  class="card-img-top"
                  alt="..."
                />
                <div class="card-body">
                  <h5 class="card-title"><%= item.name %> </h5>
                  <p class="card-text">
                    <%= item.text %> 
                  </p>
                </div>
              </div>
            </div>
            <% }) %>
            
    
        </div>
      
      </main>
    </div>
    <%- include ../partials/footer%>

    <%- include ../partials/scripts%>
  </body>
</html>
